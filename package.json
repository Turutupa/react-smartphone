    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
